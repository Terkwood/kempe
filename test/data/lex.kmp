; this is a comment

type Void {}

type Maybe a { just a | nothing }

type OS { macos | linux | windows | freebsd }

; just has type a -- Maybe a
; nothing has type -- Maybe a

osNum : OS -- Int
osNum =: [
    { case
        | macos -> 1
        | linux -> 2
        | windows -> 3
        | freebsd -> 4
    }
]

; all types are sized (monomorphized)
drop2 : a b --
drop2 =: [ drop drop ]

drop3 : a b c --
drop3 =: [ drop drop drop ]

trip : a -- a a a
trip =: [ dup dup ]
