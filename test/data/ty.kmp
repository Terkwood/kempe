type Void {}

type Maybe a { Just a | Nothing }

type OS { Macos | Linux | Windows | Freebsd }

; just has type a -- Maybe a
; nothing has type -- Maybe a

rand : -- Int
     =: $cfun"rand"

; all types are sized (monomorphized)
drop2 : a b --
      =: [ drop drop ]

drop3 : a b c --
      =: [ drop drop drop ]

trip : a -- a a a
     =: [ dup dup ]

push3 : -- OS OS OS
      =: [ Linux trip ]

trint : -- Int Int Int
      =: [ 0 trip ]

odd : Int -- Bool
    =: [ 2 % 0 = ]

randBool : -- Bool
         =: [ rand odd ]

%foreign cabi randBool
